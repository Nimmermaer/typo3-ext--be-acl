{
	"name": "p2media/be-acl",
	"description": "Backend Access Control Lists",
	"type": "typo3-cms-extension",
	"license": "GPL-2.0-or-later",
	"require": {
		"typo3/cms-core": "^10.4"
	},
	"autoload": {
		"psr-4": {
			"JBartels\\BeAcl\\": "Classes/",
			"P2Media\\BeAcl\\": "Classes/"
		}
	},
    "extra": {
        "typo3/cms": {
            "extension-key": "be_acl"
        }
    },
	"replace": {
		"be_acl": "self.version",
		"typo3-ter/be-acl": "self.version"
	}
}
